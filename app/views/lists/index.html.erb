<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path("denise-jans-OaVJQZ-nFD0-unsplash_ltwbka.jpg")%>')">
  <h1>My movie lists</h1>
</div>
<div class="container"><ul><% @lists.each do |list| %>
  <li class="index-link"><%= link_to list.name, list_path(list)%></li>
  <% if list.photo.attached? %>
    <%= cl_image_tag list.photo.key, height: 300, width: 150, crop: :thumb %>
  <% end %>
<% end %>
</ul></div>
<%= link_to "create a new list", new_list_path, class: 'btn btn-flat' %>
